{
  "name": "puffin",
  "version": "1.0.0",
  "scripts": {
    "deps": "yarn install && cd ios && pod install && cd - && cd android && ./gradlew clean assembleDebug && cd -",
    "releaseDeps": "yarn install && cd ios && pod install && cd - && cd android && ./gradlew clean assembleRelease && cd -",
    "deps:ios": "yarn install && cd ios && pod install && cd -",
    "deps:android": "cd android && ./gradlew clean app:assembleDebug && cd -",
    "start": "expo start --dev-client",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "eject": "expo eject",
    "test": "jest --watchAll",
    "prettier": "prettier --write .",
    "lint": "eslint . --ext .tsx,.ts,.js",
    "validate": "yarn lint && yarn jest --ci"
  },
  "dependencies": {
    "@expo-google-fonts/overpass": "0.2.0",
    "@react-native-async-storage/async-storage": "~1.17.3",
    "@react-native-community/netinfo": "8.2.0",
    "@react-native-picker/picker": "2.4.0",
    "@react-navigation/bottom-tabs": "6.0.5",
    "@react-navigation/native": "6.0.2",
    "@react-navigation/native-stack": "^6.6.2",
    "dotenv": "^16.0.1",
    "expo": "45.0.8",
    "expo-av": "~11.2.3",
    "expo-constants": "~13.1.1",
    "expo-dev-client": "~1.0.1",
    "expo-font": "~10.1.0",
    "expo-linear-gradient": "11.3.0",
    "expo-screen-orientation": "~4.2.0",
    "expo-splash-screen": "~0.15.1",
    "expo-status-bar": "1.3.0",
    "expo-updates": "0.13.4",
    "i18next": "^21.9.2",
    "intl-pluralrules": "^1.3.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-i18next": "^11.18.0",
    "react-native": "0.68.2",
    "react-native-safe-area-context": "4.2.4",
    "react-native-screens": "3.11.1",
    "react-native-svg": "12.4.0",
    "react-native-web": "0.17.7",
    "react-native-webrtc": "^1.100.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/react-native": "^10.1.1",
    "@types/jest": "^28.1.3",
    "@types/lodash": "^4.14.182",
    "@types/react": "~17.0.21",
    "@types/react-native": "~0.67.6",
    "@typescript-eslint/eslint-plugin": "^5.30.0",
    "@typescript-eslint/parser": "^5.30.0",
    "eas-cli": "^1.2.0",
    "eslint": "^8.18.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-universe": "^11.0.0",
    "eslint-plugin-jest": "^26.5.3",
    "eslint-plugin-jsx-a11y": "^6.6.0",
    "i18next-parser": "^6.5.0",
    "jest": "^26.6.3",
    "jest-circus": "^26.6.3",
    "jest-expo": "^45.0.1",
    "lint-staged": ">=10",
    "prettier": "^2.7.1",
    "react-test-renderer": "17",
    "typescript": "~4.3.5"
  },
  "eslintConfig": {
    "extends": "universe/native"
  },
  "private": true,
  "lint-staged": {
    "*.{tsx,ts,js}": "eslint --cache --fix",
    "*.{js,tsx,ts,md}": "prettier --write"
  },
  "resolutions": {
    "@types/react": "17.*",
    "react-devtools-core": "4.14.*",
    "react-native-svg": "12.4.0"
  }
}
